{
  "hotword": "hey",
  "command_threshold": 80,
  "processing_interval": 0.5,
  "logging": {
    "level": "INFO",
    "file": "/tmp/voice_assistant.log"
  },
  "commands": [
    {
      "name": "Terminal",
      "command": "kgx",
      "phrases": [
        "open terminal",
        "start terminal",
        "launch terminal"
      ]
    },
    {
      "name": "Firefox",
      "command": "firefox",
      "phrases": [
        "open firefox",
        "launch firefox",
        "start web browser"
      ]
    },
    {
      "name": "Files",
      "command": "nautilus",
      "phrases": [
        "open files",
        "launch files",
        "start file manager"
      ]
    },
    {
      "name": "Spotify",
      "command": "spotify",
      "phrases": [
        "open spotify",
        "launch spotify",
        "start music"
      ]
    },
    {
      "name": "VS Code",
      "command": "code",
      "phrases": [
        "open code",
        "launch code",
        "start vscode"
      ]
    },
    {
      "name": "Calculator",
      "command": "gnome-calculator",
      "phrases": [
        "open calculator",
        "launch calculator"
      ]
    },
    {
      "name": "Show Overview",
      "command": "ydotool key 125:1 125:0",
      "phrases": [
        "show overview",
        "show windows"
      ]
    },
    {
      "name": "Move Left",
      "command": "ydotool key 125:1 30:1 30:0 125:0",
      "phrases": [
        "move left",
        "go left",
        "left desktop"
      ]
    },
    {
      "name": "Move Right",
      "command": "ydotool key 125:1 32:1 32:0 125:0",
      "phrases": [
        "move right",
        "go right",
        "right desktop"
      ]
    },
    {
      "name": "Switch Window",
      "command": "ydotool key 56:1 15:1 15:0 56:0",
      "phrases": [
        "switch window",
        "next window"
      ]
    },
    {
      "name": "New Tab",
      "command": "ydotool key 29:1 15:1 15:0 29:0",
      "phrases": [
        "new tab",
        "next tab"
      ]
    },
    {
      "name": "Close Window",
      "command": "ydotool key 29:1 16:1 16:0 29:0",
      "phrases": [
        "close window",
        "close tab"
      ]
    },
    {
      "name": "Minimize",
      "command": "ydotool key 125:1 36:1 36:0 125:0",
      "phrases": [
        "minimize window",
        "minimize"
      ]
    },
    {
      "name": "Maximize",
      "command": "ydotool key 125:1 17:1 17:0 125:0",
      "phrases": [
        "maximize window",
        "maximize"
      ]
    },
    {
      "name": "Copy",
      "command": "ydotool key 29:1 46:1 46:0 29:0",
      "phrases": [
        "copy",
        "copy text"
      ]
    },
    {
      "name": "Paste",
      "command": "ydotool key 29:1 47:1 47:0 29:0",
      "phrases": [
        "paste",
        "paste text"
      ]
    },
    {
      "name": "Cut",
      "command": "ydotool key 29:1 45:1 45:0 29:0",
      "phrases": [
        "cut",
        "cut text"
      ]
    },
    {
      "name": "Undo",
      "command": "ydotool key 29:1 44:1 44:0 29:0",
      "phrases": [
        "undo",
        "undo last"
      ]
    },
    {
      "name": "Redo",
      "command": "ydotool key 29:1 21:1 21:0 29:0",
      "phrases": [
        "redo",
        "redo last"
      ]
    },
    {
      "name": "Select All",
      "command": "ydotool key 29:1 30:1 30:0 29:0",
      "phrases": [
        "select all",
        "select everything"
      ]
    },
    {
      "name": "Volume Up",
      "command": "ydotool key 115:1 115:0",
      "phrases": [
        "volume up",
        "turn up the volume",
        "increase volume"
      ]
    },
    {
      "name": "Volume Down",
      "command": "ydotool key 114:1 114:0",
      "phrases": [
        "volume down",
        "turn down the volume",
        "decrease volume"
      ]
    },
    {
      "name": "Mute",
      "command": "ydotool key 113:1 113:0",
      "phrases": [
        "mute",
        "mute audio",
        "silence"
      ]
    },
    {
      "name": "Lock Screen",
      "command": "ydotool key 125:1 46:1 46:0 125:0",
      "phrases": [
        "lock screen",
        "lock my screen"
      ]
    },
    {
      "name": "Screenshot",
      "command": "ydotool key 107:1 107:0",
      "phrases": [
        "screenshot",
        "take screenshot"
      ]
    },
    {
      "name": "Sleep",
      "command": "systemctl suspend",
      "phrases": [
        "sleep",
        "suspend"
      ]
    },
    {
      "name": "System Update",
      "command": "kgx -e yay -Syu",
      "phrases": [
        "update the system",
        "system update",
        "update system"
      ]
    }
  ],
  "typing_mode": {
    "exit_phrases": [
      "stop typing",
      "exit typing",
      "normal mode",
      "go to normal mode"
    ],
    "check_recent_chars": 100
  }
}